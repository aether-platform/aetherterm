{
  "mcpServers": {
    "supervisord-mcp": {
      "command": "uv",
      "args": [
        "run",
        "supervisord-mcp",
        "mcp"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--headless"
      ]
    },
    "mcpadvisor": {
      "command": "npx",
      "args": [
        "-y",
        "@xiaohui-wang/mcpadvisor"
      ]
    }
  }
}