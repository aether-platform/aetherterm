# AetherTerm Frontend Environment Variables
# Copy this file to .env.local and configure your values

# Application
VUE_APP_VERSION=1.0.0
NODE_ENV=development

# OpenObserve Configuration
# OpenObserve Cloud OTLP HTTP endpoint
VUE_APP_OPENOBSERVE_ENDPOINT=https://api.openobserve.ai/api/2xzmUQoXwbYU1DOjfVOknSPGOgP

# OpenObserve organization and stream names
VUE_APP_OPENOBSERVE_ORG=default
VUE_APP_OPENOBSERVE_STREAM=default

# OpenObserve authentication (Basic Auth)
VUE_APP_OPENOBSERVE_USERNAME=kaz@re-x.info
VUE_APP_OPENOBSERVE_PASSWORD=c8KGNeJfJIbZqFu7

# Development Features
VUE_APP_ENABLE_JWT_DEV_REGISTER=true

# OpenTelemetry Configuration
# Alternative OTLP endpoint (if using different collector)
VUE_APP_OTEL_ENDPOINT=http://localhost:4318

# Sample rates for production (0.0 to 1.0)
VUE_APP_TRACE_SAMPLE_RATE=1.0
VUE_APP_ERROR_SAMPLE_RATE=1.0

# Service Information
VUE_APP_SERVICE_NAME=aetherterm-frontend
VUE_APP_SERVICE_NAMESPACE=aetherterm

# Debug Settings
VUE_APP_DEBUG_TELEMETRY=true
VUE_APP_CONSOLE_LOG_LEVEL=debug

# Enable OpenTelemetry features
VUE_APP_ENABLE_OTLP_EXPORT=true