{
  "dashboard": {
    "id": null,
    "title": "AetherTerm Socket.IO Tracing",
    "tags": ["aetherterm", "socketio", "tracing"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Socket.IO Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(socketio_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Socket.IO Response Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(socketio_duration_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(socketio_duration_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Frontend-Backend Trace Correlation",
        "type": "table",
        "targets": [
          {
            "queryType": "logs",
            "expr": "{service_name=\"aetherterm-frontend\"} |= \"traceId\"",
            "legendFormat": "Frontend Traces"
          },
          {
            "queryType": "logs", 
            "expr": "{service_name=\"aetherterm-backend\"} |= \"traceId\"",
            "legendFormat": "Backend Traces"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "traceId": "Trace ID",
                "operationName": "Operation",
                "duration": "Duration (ms)",
                "status": "Status"
              }
            }
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Socket.IO Events by Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (event_name) (rate(socketio_events_total[5m]))",
            "legendFormat": "{{event_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(socketio_errors_total[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 1}
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 6,
        "title": "Terminal Operations Timeline",
        "type": "timeseries",
        "targets": [
          {
            "queryType": "traces",
            "expr": "terminal:create",
            "legendFormat": "Terminal Create"
          },
          {
            "queryType": "traces",
            "expr": "terminal:input", 
            "legendFormat": "Terminal Input"
          },
          {
            "queryType": "traces",
            "expr": "terminal:resize",
            "legendFormat": "Terminal Resize"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "Active Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "socketio_active_sessions",
            "legendFormat": "Active Sessions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 32}
      },
      {
        "id": 8,
        "title": "Connected Clients",
        "type": "stat",
        "targets": [
          {
            "expr": "socketio_connected_clients",
            "legendFormat": "Connected Clients"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 20},
                {"color": "red", "value": 100}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 32}
      },
      {
        "id": 9,
        "title": "Request-Response Correlation Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "(sum(rate(socketio_correlations_success_total[5m])) / sum(rate(socketio_correlations_total[5m]))) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 32}
      },
      {
        "id": 10,
        "title": "Slow Requests",
        "type": "logs",
        "targets": [
          {
            "queryType": "logs",
            "expr": "{service_name=\"aetherterm-backend\"} |= \"slow\" | duration > 1000",
            "legendFormat": "Slow Requests (>1s)"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 36}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(service_name)",
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true
        },
        {
          "name": "event_type",
          "type": "query", 
          "query": "label_values(socketio_event_name)",
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "OpenObserve",
          "enable": true,
          "expr": "{service_name=\"aetherterm-backend\"} |= \"deployment\""
        }
      ]
    }
  }
}